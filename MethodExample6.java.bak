class MethodExample
{
	public void percentage(){
		int num1 =3;
		int num2 =6;
		int res  =num2+num1/2;
		System.out.println("First number--- "+num1);
		System.out.println("Second number--- "+num2);
		System.out.println("Result---- "+res);
	}
	public static void main(String[] args) 
	{
		MethodExample m = new MethodExample();
		m.percentage();
	}
}
